
{
    "manifest_version": 3,
    "name": "Right Click Custom Menu",
    "version": "1.0",
    "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "downloads", "contextMenus", "tabs", "tabGroups","storage"],
  "content_scripts": [
    {
        "matches": [ "<all_urls>" ],
        "js": [ "content.js" ],
        "run_at": "document_start",
        "all_frames": true
    }
]
  
  
}


